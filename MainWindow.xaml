<Window x:Class="stankin1client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:stankin1client"
        mc:Ignorable="d"
        Title="Stankin1" Height="388" Width="800">
    <Grid Margin="0,0,0,7">
        <Button x:Name="button" Content="Load students" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="32" Width="125" Click="button_Click"/>
        <DataGrid x:Name="dataGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,47,20,86" IsEnabled="False"/>
    </Grid>
</Window>
